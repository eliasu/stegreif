    <div x-data="sending" x-cloak>
        <form method="POST" action="http://stegreif.test/!/forms/contact" x-data="{&quot;dynamic_form&quot;:{&quot;name&quot;:null,&quot;email&quot;:null}}" id="form-form" x-ref="form" @submit.prevent="sendForm()">
